{
  "name": "@playvs-inc/nexus-storybook",
  "version": "1.1.0",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src/*",
    "lib/*"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc",
    "release": "publish-if-not-published",
    "prepublishOnly": "yarn install && yarn run build",
    "build:watch": "tsup --watch",
    "yalc:watch": "tsup src/index.ts --watch --onSuccess \"yalc push\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": "@nexus/prettier-config",
  "peerDependencies": {},
  "dependencies": {
    "native-base": "^3.4.25",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-native": "^0.71.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-svg": "12.1.1",
    "@storybook/react": "^6.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@nexus/eslint-config": "^0.1.2",
    "@nexus/prettier-config": "^0.1.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.3.0",
    "babel-plugin-react-native-web": "^0.18.12",
    "esbuild": "^0.14.17",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "prettier": "^2.5.1",
    "publish-if-not-published": "^3.1.3",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.18.12",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "tsconfig": "./tsconfig.json",
    "outDir": "./lib",
    "legacyOutput": true
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:playvs-inc/nexus.git",
    "directory": "@playvs-inc/nexus-storybook"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/playvs-inc"
  }
}
